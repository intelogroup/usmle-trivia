  "automationRules": {
    "healthMetrics": {
      "lighthouseScore": {
        "threshold": 90,
        "belowThreshold": ["performance", "accessibility"]
      },
      "testFailures": {
        "threshold": 0,
        "aboveThreshold": ["testing", "code-quality"]
      },
      "bundleSize": {
        "threshold": 500,
        "aboveThreshold": ["performance", "code-quality"]
      },
      "accessibilityScore": {
        "threshold": 90,
        "belowThreshold": ["accessibility", "testing"]
      }
    },
    
